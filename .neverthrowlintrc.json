{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "rules": [
    {
      "name": "Service layer must use neverthrow Result type",
      "path": "electron/module/**/service.ts",
      "enforceResult": true,
      "apply": "async-functions",
      "exceptions": [
        "getAppVersion",
        "clearMigrationCache",
        "clearAllStoredSettings",
        "clearStoredSetting",
        "getApplicationLogPath",
        "installUpdate",
        "handlePngBase64WithCallback",
        "showSavePngDialog",
        "saveFileToPath",
        "getStoredVRChatLogFilesDirPath",
        "getVRChatLogFileDir",
        "createVRChatWorldJoinLogFromPhoto",
        "findVRChatWorldJoinLogFromPhotoList",
        "getVRChaLogInfoByLogFilePathListWithPartialSuccess",
        "findAllVRChatWorldJoinLogList",
        "findVRChatWorldJoinLogList",
        "performMigrationIfNeeded",
        "isMigrationNeeded",
        "syncLogsInBackground",
        "findNextVRChatWorldJoinLog",
        "findRecentVRChatWorldJoinLog",
        "_getLogStoreFilePaths",
        "_getLogStoreFilePathsImpl",
        "getOldAppUserDataPath",
        "processQueue",
        "copyImageByBase64",
        "getVrcUserInfoByUserName",
        "newLogs",
        "latestPhotoDate",
        "importResult"
      ]
    }
  ]
}
