{
  "name": "vrchat-photo-journey",
  "version": "0.1.0",
  "main": "main/index.js",
  "author": {
    "name": "tktcorporation"
  },
  "description": "",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/tktcorporation/add-world-name-to-vrc-photo"
  },
  "productName": "vrchat-photo-journey",
  "scripts": {
    "preinstall": "nlx only-allow yarn",
    "postinstall": "prisma generate && simple-git-hooks && nr license-check:generate",
    "setup:debug": "bash debug/genDevFiles.sh",
    "dev": "./scripts/dev.sh",
    "dev:vite": "vite",
    "dev:electron": "nr build:electron && electron . --disable-gpu",
    "build": "nr build:vite && nr build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p electron",
    "dist": "nr clean:build && nr build && electron-builder --publish never",
    "pack": "nr clean:build && nr build && electron-builder --dir",
    "clean:build": "rimraf dist main src/out",
    "clean:debug": "bash debug/cleanDevFiles.sh",
    "find-deadcode": "ts-prune",
    "license-check:generate": "license-checker --production --json --out src/assets/licenses.json",
    "license-check:validate": "license-checker --production --failOn 'GPL;GPL-2.0;GPL-3.0;LGPL;LGPL-3.0;AGPL;AGPL-3.0;AGPL-3.0-only;'",
    "test": "jest",
    "test:playwright": "playwright install --with-deps && playwright test",
    "lint": "nr biome check . && nr lint:type-check",
    "lint:type-check": "nr lint:type-check:electron && nr lint:type-check:src",
    "lint:type-check:electron": "tsc --noEmit -p electron",
    "lint:type-check:src": "tsc --noEmit",
    "lint:fix": "nr biome check --apply ."
  },
  "simple-git-hooks": {
    "pre-commit": "nr lint"
  },
  "engines": {
    "node": "20",
    "yarn": "4"
  },
  "dependencies": {
    "@heroicons/react": "2.1.3",
    "@prisma/client": "5.13.0",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-label": "2.0.2",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-scroll-area": "1.0.5",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "1.0.3",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-tooltip": "1.0.7",
    "@sequelize/core": "7.0.0-alpha.40",
    "@sequelize/sqlite3": "7.0.0-alpha.40",
    "@tanstack/react-query": "4.36.1",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@vitejs/plugin-react": "4.2.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.1",
    "electron-is-dev": "2.0.0",
    "electron-log": "5.1.2",
    "electron-store": "8.2.0",
    "electron-trpc": "0.5.2",
    "electron-unhandled": "4.0.1",
    "i": "0.3.7",
    "iconv-lite": "0.6.3",
    "jschardet": "3.1.2",
    "lucide-react": "0.376.0",
    "neverthrow": "6.2.1",
    "opentype.js": "1.3.4",
    "pony-cause": "2.1.11",
    "prisma": "5.13.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "react-hot-toast": "2.4.1",
    "react-intersection-observer": "9.8.2",
    "react-router-dom": "6.23.0",
    "sharp": "0.33.3",
    "superjson": "1.13.3",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "ts-pattern": "5.1.1",
    "vite": "5.2.10",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.0",
    "@playwright/test": "1.43.1",
    "@swc/core": "1.4.17",
    "@swc/jest": "0.2.36",
    "@types/babel__core": "7.20.5",
    "@types/electron-is-dev": "1.1.1",
    "@types/jest": "29.5.12",
    "@types/license-checker": "25.0.6",
    "@types/node": "20.12.7",
    "@types/opentype.js": "1.3.8",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/sharp": "0.32.0",
    "@welldone-software/why-did-you-render": "8.0.1",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "electron": "27.3.11",
    "electron-builder": "24.13.3",
    "jest": "29.7.0",
    "license-checker": "25.0.1",
    "playwright": "1.43.1",
    "postcss": "8.4.38",
    "rimraf": "5.0.5",
    "simple-git-hooks": "2.11.1",
    "ts-prune": "0.10.3",
    "typescript": "5.4.5"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "src/out"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "extraResources": [
      {
        "from": "./assets/",
        "to": "assets"
      },
      "prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*",
      "node_modules/.bin/prisma"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
