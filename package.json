{
  "name": "vrchat-photo-journey",
  "version": "0.11.0",
  "main": "main/index.cjs",
  "author": {
    "name": "tktcorporation"
  },
  "description": "",
  "keywords": [
    "vite",
    "react",
    "electron",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/tktcorporation/add-world-name-to-vrc-photo"
  },
  "productName": "vrchat-photo-journey",
  "type": "module",
  "scripts": {
    "preinstall": "nlx only-allow yarn",
    "postinstall": "simple-git-hooks && nr license-check:generate",
    "setup:debug": "bash debug/genDevFiles.sh",
    "dev": "./scripts/dev.sh",
    "dev:vite": "vite",
    "dev:electron": "vite build -c electron/vite.config.ts && electron /workspaces/vrchat-photo-journey --disable-gpu",
    "build": "nr lint:type-check && nr build:vite && nr build:electron",
    "build:vite": "vite build",
    "build:electron": "vite build -c electron/vite.config.ts",
    "build:electron:tsc": "tsc -p electron",
    "build:electron:watch": "vite build -c electron/vite.config.ts --watch",
    "dist": "nr clean:build && nr build && NODE_ENV=production electron-builder --config=electron-builder.cjs --publish never",
    "pack": "nr clean:build && nr build && NODE_ENV=production electron-builder --config=electron-builder.cjs --dir",
    "publish": "nr clean:build && nr build && NODE_ENV=production electron-builder --config=electron-builder.cjs --publish always",
    "clean:build": "rimraf dist main src/out",
    "update-changelog": "git-cliff -c ./cliff.toml -o CHANGELOG.md",
    "clean:debug": "bash debug/cleanDevFiles.sh",
    "find-deadcode": "ts-prune",
    "license-check:generate": "license-checker --production --json --out src/assets/licenses.json",
    "license-check:validate": "license-checker --production --failOn 'GPL;GPL-2.0;GPL-3.0;LGPL;LGPL-3.0;AGPL;AGPL-3.0;AGPL-3.0-only;'",
    "test": "nr vitest run",
    "test:web": "vitest run --project web",
    "test:electron": "vitest run --project electron",
    "jest": "jest --runInBand",
    "test:playwright": "playwright install --with-deps && playwright test",
    "check-unused": "npx tsr 'electron/index\\.ts' 'src/main\\.tsx' '\\.config\\.ts' 'spec.ts' 'test.ts'",
    "lint": "nr lint:biome && nr lint:type-check",
    "lint:biome": "biome check .",
    "lint:type-check": "nr lint:type-check:electron && nr lint:type-check:src",
    "lint:type-check:electron": "tsc --noEmit -p electron",
    "lint:type-check:src": "tsc --noEmit",
    "lint:fix": "biome check --apply-unsafe . && nr lint:type-check",
    "sentry:sourcemaps": "nr sentry:sourcemaps:electron && nr sentry:sourcemaps:vite",
    "sentry:sourcemaps:electron": "sentry-cli sourcemaps inject --org $SENTRY_ORG --project $SENTRY_PROJECT ./main && sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT ./main",
    "sentry:sourcemaps:vite": "sentry-cli sourcemaps inject --org $SENTRY_ORG --project $SENTRY_PROJECT ./src/out && sentry-cli sourcemaps upload --org $SENTRY_ORG --project $SENTRY_PROJECT ./src/out"
  },
  "simple-git-hooks": {
    "pre-commit": "nr lint"
  },
  "packageManager": "yarn@4.1.0",
  "engines": {
    "node": "20",
    "yarn": "4"
  },
  "dependencies": {
    "@heroicons/react": "2.1.3",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-context-menu": "^2.2.4",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-progress": "1.0.3",
    "@radix-ui/react-scroll-area": "^1.2.2",
    "@radix-ui/react-slot": "1.0.2",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "1.0.4",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-tooltip": "1.0.7",
    "@sentry/cli": "^2.40.0",
    "@sentry/electron": "^5.9.0",
    "@sentry/vite-plugin": "^2.22.7",
    "@sequelize/core": "7.0.0-alpha.40",
    "@sequelize/sqlite3": "7.0.0-alpha.40",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-virtual": "^3.11.2",
    "@trpc/client": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@vitejs/plugin-react": "4.2.1",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "consola": "^3.2.3",
    "date-fns": "2.30.0",
    "date-fns-tz": "2.0.1",
    "electron-is-dev": "2.0.0",
    "electron-log": "5.3.0",
    "electron-store": "8.2.0",
    "electron-trpc": "0.5.2",
    "electron-unhandled": "4.0.1",
    "electron-updater": "6.3.9",
    "exiftool-vendored": "^24.5.0",
    "framer-motion": "11.2.4",
    "glob": "10.4.3",
    "i": "0.3.7",
    "iconv-lite": "0.6.3",
    "jschardet": "3.1.2",
    "lucide-react": "0.376.0",
    "neverthrow": "6.2.1",
    "ofetch": "^1.4.1",
    "opentype.js": "1.3.4",
    "p-queue": "^8.1.0",
    "pathe": "1.1.2",
    "pony-cause": "2.1.11",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.13",
    "react-hot-toast": "2.4.1",
    "react-intersection-observer": "9.8.2",
    "react-router-dom": "6.23.0",
    "react-virtuoso": "4.7.2",
    "rxjs": "^7.8.1",
    "sharp": "0.33.3",
    "superjson": "1.13.3",
    "tailwind-merge": "2.3.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "1.0.7",
    "tmp-promise": "^3.0.3",
    "ts-pattern": "5.1.1",
    "ufo": "^1.5.4",
    "undici": "^7.3.0",
    "uuidv7": "^1.0.2",
    "vaul": "0.9.0",
    "vite": "5.4.14",
    "zod": "3.23.4",
    "zustand": "4.5.2"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.0",
    "@biomejs/biome": "1.6.0",
    "@playwright/test": "1.43.1",
    "@swc/core": "1.4.17",
    "@swc/jest": "0.2.36",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/babel__core": "7.20.5",
    "@types/electron-is-dev": "1.1.1",
    "@types/jest": "29.5.12",
    "@types/license-checker": "25.0.6",
    "@types/node": "20.12.7",
    "@types/opentype.js": "1.3.8",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "@types/sharp": "0.32.0",
    "@welldone-software/why-did-you-render": "^8.0.3",
    "autoprefixer": "10.4.19",
    "concurrently": "8.2.2",
    "electron": "34.3.0",
    "electron-builder": "24.13.3",
    "git-cliff": "^2.5.0",
    "jest": "29.7.0",
    "jsdom": "^25.0.1",
    "license-checker": "25.0.1",
    "playwright": "1.43.1",
    "postcss": "8.4.38",
    "rimraf": "5.0.5",
    "simple-git-hooks": "2.11.1",
    "ts-prune": "0.10.3",
    "typescript": "5.7.2",
    "vitest": "^2.1.8"
  },
  "build": {
    "productName": "vrchat-photo-index",
    "publish": {
      "provider": "github",
      "owner": "tktcorporation",
      "repo": "vrchat-photo-journey"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "assets/icons"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/icon.icns"
    }
  }
}
